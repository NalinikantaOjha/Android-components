<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.masai.androidactivity">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AndroidActivity">
        <service
            android:name=".IntentService.MyIntentService"
            android:exported="false"></service>
        <service
            android:name=".services.service.local.MyService"
            android:enabled="true"
            android:exported="true" />

        <activity
            android:name=".services.service.ui.ServiceActivityDestroyWithApp"
            android:exported="true" />
        <activity
            android:name=".BoundService.ui.BoundServiceActivity"
            android:exported="true" />
        <activity
            android:name=".IntentService.IntentServiceActivity"
            android:exported="true" />
        <activity
            android:name=".services.ServiceActivity"
            android:exported="true" />

        <service
            android:name=".services.ForegroundService.local.MyService"
            android:enabled="true"
            android:exported="true" />

        <activity
            android:name=".services.ForegroundService.ui.ForegroundActivity"
            android:exported="true" />
        <activity
            android:name=".ui.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>